import java.util.Scanner;
class Palindrome{
public static void main(String[] args){
System.out.println("enter the number");
Scanner n=new Scanner(System.in);
int a=n.nextInt();
int rem=0;
int temp=a;
int rev=rem;
while(temp!=0){
rem=temp%10;
rev=(rev*10)+rem;
temp=temp/10;
}
if(a==rev){
System.out.println("it is a palindrome");
}
else{
System.out.println("sit is not a palindrome");
}
}
}
